GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LGHelper" "${PODS_ROOT}/Headers/Public/Reachability" "${PODS_ROOT}/Headers/Public/StreamingKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LGHelper" -isystem "${PODS_ROOT}/Headers/Public/Reachability" -isystem "${PODS_ROOT}/Headers/Public/StreamingKit"
OTHER_LDFLAGS = $(inherited) -ObjC -l"LGHelper" -l"Reachability" -l"StreamingKit" -framework "AudioToolbox" -framework "CFNetwork" -framework "CoreFoundation" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods